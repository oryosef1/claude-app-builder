{
  "name": "claude-dashboard",
  "version": "1.0.0",
  "description": "Web dashboard for Claude automated workflow system",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose unit/",
    "test:integration": "vitest run --reporter=verbose integration/",
    "test:e2e": "vitest run --reporter=verbose e2e/",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mui/material": "^5.14.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "react-router-dom": "^6.14.0",
    "zustand": "^4.4.0",
    "socket.io-client": "^4.7.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "typescript": "^5.2.0",
    "vite": "^4.4.0",
    "vitest": "^0.34.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.46.0",
    "prettier": "^3.0.0",
    "@vitest/coverage-v8": "^0.34.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.4.0",
    "jsdom": "^22.1.0"
  }
}