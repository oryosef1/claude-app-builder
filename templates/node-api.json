{
  "name": "Node.js API",
  "description": "RESTful API with Express, TypeScript, and MongoDB",
  "structure": {
    "src/": {
      "controllers/": ["auth.controller.ts", "user.controller.ts"],
      "models/": ["User.ts", "Project.ts"],
      "routes/": ["auth.routes.ts", "user.routes.ts"],
      "middleware/": ["auth.middleware.ts", "validation.middleware.ts"],
      "services/": ["auth.service.ts", "database.service.ts"],
      "types/": ["express.d.ts", "user.types.ts"],
      "utils/": ["logger.ts", "helpers.ts", "constants.ts"],
      "config/": ["database.ts", "environment.ts"]
    },
    "tests/": {
      "unit/": [],
      "integration/": [],
      "e2e/": []
    }
  },
  "dependencies": [
    "express@^4.18.0",
    "mongoose@^7.0.0",
    "jsonwebtoken@^9.0.0",
    "bcryptjs@^2.4.0",
    "cors@^2.8.0",
    "helmet@^7.0.0",
    "express-rate-limit@^6.0.0"
  ],
  "devDependencies": [
    "typescript@^5.0.0",
    "@types/express@^4.17.0",
    "@types/node@^20.0.0",
    "@types/jsonwebtoken@^9.0.0",
    "@types/bcryptjs@^2.4.0",
    "nodemon@^3.0.0",
    "jest@^29.0.0",
    "supertest@^6.0.0"
  ],
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext ts"
  }
}