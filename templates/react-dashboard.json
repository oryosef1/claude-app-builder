{
  "name": "React Dashboard",
  "description": "Modern React dashboard with TypeScript, Vite, and Material-UI",
  "structure": {
    "src/": {
      "components/": {
        "common/": ["Button.tsx", "Input.tsx", "Modal.tsx"],
        "layout/": ["Header.tsx", "Sidebar.tsx", "Footer.tsx"],
        "dashboard/": ["DashboardCard.tsx", "MetricsPanel.tsx"]
      },
      "pages/": ["Dashboard.tsx", "Settings.tsx", "Projects.tsx"],
      "hooks/": ["useApi.ts", "useWebSocket.ts", "useLocalStorage.ts"],
      "services/": ["api.ts", "websocket.ts", "storage.ts"],
      "types/": ["api.ts", "workflow.ts", "user.ts"],
      "utils/": ["helpers.ts", "constants.ts", "validators.ts"],
      "styles/": ["globals.css", "variables.css"]
    },
    "public/": ["favicon.ico", "logo.svg"],
    "tests/": {
      "unit/": [],
      "integration/": [],
      "e2e/": []
    }
  },
  "dependencies": [
    "react@^18.0.0",
    "react-dom@^18.0.0",
    "@mui/material@^5.0.0",
    "@emotion/react@^11.0.0",
    "@emotion/styled@^11.0.0",
    "react-router-dom@^6.0.0",
    "zustand@^4.0.0"
  ],
  "devDependencies": [
    "typescript@^5.0.0",
    "vite@^4.0.0",
    "vitest@^0.32.0",
    "@types/react@^18.0.0",
    "@types/react-dom@^18.0.0",
    "eslint@^8.0.0",
    "prettier@^3.0.0"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "preview": "vite preview"
  }
}